package view.dessinable;

import java.awt.*;
import java.awt.geom.AffineTransform;

/**
 * Interface qui défini les méthodes que les classes dessinables doivent implémenter
 * @author Simon Tran
 */
public interface Dessinable {

    /**
     * Permet de dessiner le composant
     * @param g2d   Le Graphics2D
     * @param transform La matrice de transformations
     */
    void draw(Graphics2D g2d, AffineTransform transform);
}
